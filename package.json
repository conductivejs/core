{
  "name": "@conductive/core",
  "version": "0.0.16",
  "description": "Elegant component framework for Express.js",
  "type": "module",
  "main": "./bin/cjs/core.js",
  "module": "./bin/mjs/core.js",
  "exports": {
    ".": {
      "require": "./bin/cjs/core.js",
      "import": "./bin/mjs/core.js"
    },
    "./errors": {
      "require": "./bin/cjs/errors.js",
      "import": "./bin/mjs/errors.js"
    },
    "./middleware": {
      "require": "./bin/cjs/middleware.js",
      "import": "./bin/mjs/middleware.js"
    }
  },
  "scripts": {
    "lint": "eslint",
    "build": "rm -rf bin && rollup --config",
    "deploy": "npm run build && npm publish --access public"
  },
  "author": {
    "name": "Aeden Murray",
    "email": "aeden@aedenmurray.io",
    "url": "https://aedenmurray.io"
  },
  "homepage": "https://conductive.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/conductivejs/core"
  },
  "keywords": [
    "express",
    "framework",
    "component",
    "mvc",
    "web"
  ],
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "ajv": "^8.8.2",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "express": "^4.17.2",
    "helmet": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "rollup": "^2.63.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "files": [
    "bin/**"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 14
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
