  "description": "Elegant component framework for Express.js",
  "main": "./core.js",
  "exports": {
    ".": "./core.js",
    "./errors": "./errors.js",
    "./middleware": "./middleware.js"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "author": {
    "name": "Aeden Murray",
    "email": "aeden@aedenmurray.io",
    "url": "https://aedenmurray.io"
  },
